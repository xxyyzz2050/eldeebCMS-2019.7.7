{
  "name": "eldeeb-cms",
  "version": "0.0.1",
  "scripts": {
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "mobile": "tns run --bundle",
    "preview": "tns preview --bundle",
    "start": "npm run build  && npm run serve:firebase",
    "start-dev": "npm run build-dev  && npm run serve:firebase",
    "build": "npm run build:browser && npm run build:server",
    "build-dev": "npm run build:browser-dev && npm run build:server-dev",
    "build:browser": "ng build --prod",
    "build:browser-dev": "ng build --prod=false",
    "build:server": "ng run eldeebCMS:server:production && npm run webpack:server",
    "build:server-dev": "ng run eldeebCMS:server && npm run webpack:server",
    "build:firebase": "npm --prefix ./firebase run build --prod",
    "build:firebase-dev": "npm --prefix ./firebase run build",
    "webpack:server": "npx webpack --config webpack.server.config.js --progress --colors",
    "serve:server": "nodemon dist/server --inspaect --open --watch",
    "serve:browser": "ng serve --open --watch",
    "serve:firebase": "npm run serve:server | npx firebase serve",
    "serve": "npm run serve:server",
    "test": "ng test",
    "e2e": "ng e2e",
    "code": "npm run code:prettier && npm run code:tslint",
    "code:prettier": "npx prettier --write --config=prettier.config.json \"**/*.{js,ts,css,scss,json,json5,md,html,yaml}\"",
    "code:tslint": "npx tslint --fix \"**\"",
    "deploy:firebase": "npx firebase deploy",
    "doc": "npx compodoc -p package.json"
  },
  "private": false,
  "dependencies": {
    "nativescript-angular": "~7.2.0",
    "nativescript-theme-core": "~1.0.4",
    "reflect-metadata": "~0.1.12",
    "tns-core-modules": "~5.2.0",
    "@angular/animations": "~8.0.1",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.1",
    "@angular/material": "^8.0.1",
    "@angular/material-moment-adapter": "^8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/platform-server": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@compodoc/compodoc": "^1.1.9",
    "@nativescript/schematics": "^0.6.0",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "rxjs": "~6.4.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "nativescript-dev-webpack": "^0.20.0",
    "@nativescript/schematics": "~0.4.0",
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.0.10",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.18.0",
    "typescript": "~3.4.3",
    "webpack-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxyyzz2050/eldeebCMS"
  },
  "author": "eldeeb<facebook.com/eng.dibo>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xxyyzz2050/eldeebCMS/issues"
  },
  "homepage": "https://github.com/xxyyzz2050/eldeebCMS#readme",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "compodoc": {
    "output": "dist/docs",
    "tsconfig": "tsconfig.json",
    "open": true,
    "port": 4200,
    "watch": true,
    "exportFormat": "html",
    "language": "en-US",
    "theme": "material",
    "hideGenerator": true,
    "gaID": ""
  },
  "nativescript": {
    "id": "org.nativescript.ngsample"
  }
}
